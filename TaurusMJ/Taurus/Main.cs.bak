        private ToolStripMenuItem GetModuleItem(string ModuleName, ref bool state)
        {
            state = false;
            ToolStripMenuItem ret = null;
            ToolStripItem[] FindItem = MenuBar.Items.Find("mnu" + ModuleName, true);
            if (FindItem.Length == 1)
            {
                ret = (ToolStripMenuItem)FindItem[0];
                state = ret.Enabled;
            }
            return ret;
        }

        public void ExecModule(string ModuleName, string Module)
        {
            ToolStripMenuItem item;
            bool state = false;
            switch (Module.ToUpper())
            {
                case "":
                case "HELP":
                    switch (ModuleName)
                    {
                        case "PubReportLog":
                            ShowPubReportLog();
                            break;
                        case "HelpTopic":
                            ShowHelpTopic();
                            break;
                        case "HelpAbout":
                            ShowAbout();
                            break;
                    }
                    break;
                 case "SY":
                    switch (ModuleName)
                    {
                        case "SYPower":
                            ShowSYPower();
                            break;
                        case "SYPassword":
                            ShowSYPassword();
                            break;
                        case "SYDataBack":
                            ShowSYDataBack();
                            break;
                        case "SYDataRest":
                            ShowSYDataRest();
                            break;
                        case "SYDataUpdate":
                            ShowSYDataUpdate();
                            break;
                        case "SYDataCompact":
                            ShowSYDataCompact();
                            break;
                        case "SYDataType":
                            ShowSYDataType();
                            break;
                        case "SYDataClear":
                            ShowSYDataClear();
                            break;
                        case "SYOption":
                            ShowSYOption();
                            break;
                        case "KQInOutMode":
                            ShowKQ(ModuleName);
                            break;
                    }
                    break;
                case "RS":
                    item = GetModuleItem(ModuleName, ref state);
                    if (item != null)
                    {
                        if (!state) return;
                        ShowRS(item);
                    }
                    else
                        ShowRS(ModuleName);
                    break;
                case "MJ":
                    item = GetModuleItem(ModuleName, ref state);
                    if (item != null)
                    {
                        if (!state) return;
                        ShowMJ(item);
                    }
                    else
                        ShowMJ(ModuleName);
                    break;
                case "KQ":
                    item = GetModuleItem(ModuleName, ref state);
                    if (item != null)
                    {
                        if (!state) return;
                        ShowKQ(item);
                    }
                    else
                        ShowKQ(ModuleName);
                    break;
                case "GZ":
                    item = GetModuleItem(ModuleName, ref state);
                    if (item != null)
                    {
                        if (!state) return;
                        ShowGZ(item);
                    }
                    else
                        ShowGZ(ModuleName);
                    break;
                case "EXT":
                    switch (ModuleName.ToLower())
                    {
                        case "about":
                            ShowAbout();
                            break;
                        case "exit":
                            this.Close();
                            break;
                    }
                    break;
            }
        }


        private void InitToolbarButton()
        {
            FuncObject funcObj;
            FuncSubObject funcSubObj;
            bool HasRS = false;
            bool HasMJ = false;
            bool HasKQ = false;
            bool HasGZ = false;
            bool IsAddLineRS = false;
            bool IsAddLineMJ = false;
            bool IsAddLineKQ = false;
            bool IsAddLineGZ = false;
            for (int i = 0; i < SystemInfo.funcList.Count; i++)
            {
                funcObj = SystemInfo.funcList[i];
                for (int j = 0; j < funcObj.SubCount; j++)
                {
                    funcSubObj = SystemInfo.funcList[i].SubGet(j);
                    if (funcSubObj.IsLine != 2 && funcSubObj.IsTool)
                    {
                        if (funcObj.Name == "RS")
                        {
                            HasRS = true;
                            AddToolbarButton(funcSubObj.Text, funcSubObj.Name, mnuRS_Click);
                        }
                        if (funcObj.Name == "MJ")
                        {
                            if (HasRS && !IsAddLineRS)
                            {
                                IsAddLineRS = true;
                                AddToolbarSeparator();
                            }
                            HasMJ = true;
                            AddToolbarButton(funcSubObj.Text, funcSubObj.Name, mnuMJ_Click);
                        }
                        if (funcObj.Name == "KQ")
                        {
                            if ((HasRS && !IsAddLineRS) || (HasMJ && !IsAddLineMJ))
                            {
                                IsAddLineRS = true;
                                IsAddLineMJ = true;
                                AddToolbarSeparator();
                            }
                            HasKQ = true;
                            AddToolbarButton(funcSubObj.Text, funcSubObj.Name, mnuKQ_Click);
                        }
                        if (funcObj.Name == "GZ")
                        {
                            if ((HasRS && !IsAddLineRS) || (HasMJ && !IsAddLineMJ) || (HasKQ && !IsAddLineKQ))
                            {
                                IsAddLineRS = true;
                                IsAddLineMJ = true;
                                IsAddLineKQ = true;
                                AddToolbarSeparator();
                            }
                            HasGZ = true;
                            AddToolbarButton(funcSubObj.Text, funcSubObj.Name, mnuGZ_Click);
                        }
                    }
                }
            }
            if ((HasRS && !IsAddLineRS) || (HasMJ && !IsAddLineMJ) || (HasKQ && !IsAddLineKQ) || (HasGZ && !IsAddLineGZ))
            {
                IsAddLineRS = true;
                IsAddLineMJ = true;
                IsAddLineKQ = true;
                IsAddLineGZ = true;
                AddToolbarSeparator();
            }
        }

